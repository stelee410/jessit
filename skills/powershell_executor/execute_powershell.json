{
  "name": "execute_powershell",
  "description": "执行PowerShell命令并返回结果",
  "parameters": {
    "type": "object",
    "properties": {
      "command": {
        "type": "string",
        "description": "要执行的PowerShell命令"
      },
      "timeout": {
        "type": "integer",
        "description": "命令执行超时时间（秒），默认为30秒",
        "default": 30
      },
      "working_directory": {
        "type": "string",
        "description": "执行命令的工作目录，默认为当前目录"
      }
    },
    "required": ["command"]
  },
  "handler": "powershell.execute",
  "enabled": true
}
